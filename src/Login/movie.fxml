<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Login.MovieController">
   <children>
      <ImageView fitHeight="300.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../../../../ojdbc6/black.png" />
         </image>
      </ImageView>
      <MediaView fx:id="fxMediaView" fitHeight="180.0" fitWidth="450.0" layoutX="27.0" layoutY="71.0" />
      <Button layoutX="147.0" layoutY="23.0" mnemonicParsing="false" onAction="#myPlay" prefHeight="30.0" prefWidth="230.0" style="-fx-background-color: black;" text="Today's best movie" textFill="WHITE">
         <font>
            <Font size="24.0" />
         </font></Button>
      <ImageView fitHeight="55.0" fitWidth="267.0" layoutX="126.0" layoutY="22.0" onMouseClicked="#myPlay" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../../../../ojdbc6/play.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="160.0" layoutY="259.0" onMouseClicked="#reservation" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../../../../ojdbc6/reservation.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
