<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kjh_Manager.movieInfo.MovieAddController">
   <children>
      <ImageView fx:id="movieImage" fitHeight="226.0" fitWidth="157.0" layoutX="73.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true" />
      <TextField fx:id="movieName" layoutX="81.0" layoutY="320.0" prefHeight="30.0" promptText="영화 제목">
         <font>
            <Font name="MoeumT R" size="12.0" />
         </font>
      </TextField>
      <TextField fx:id="movieAvg" layoutX="81.0" layoutY="368.0" prefHeight="30.0" promptText="영화 평점">
         <font>
            <Font name="MoeumT R" size="12.0" />
         </font>
      </TextField>
      <Button layoutX="124.0" layoutY="410.0" mnemonicParsing="false" onAction="#movieAdd" text="완료">
         <font>
            <Font name="MoeumT R" size="18.0" />
         </font>
      </Button>
      <Button layoutX="220.0" layoutY="459.0" mnemonicParsing="false" onAction="#back" text="돌아가기">
         <font>
            <Font name="MoeumT R" size="13.0" />
         </font>
      </Button>
      <Button layoutX="81.0" layoutY="269.0" mnemonicParsing="false" onAction="#imageAdd" prefHeight="30.0" prefWidth="140.0" text="이미지 추가">
         <font>
            <Font name="MoeumT R" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
